  <div class="container">
    <div class="row mx-auto d-block mt-5">
      <p class ="h3 text-center">検索結果</p>
    </div>
    <table class="table table-hover table-inverse text-center">
    <!--検索対象モデルがraceの時 -->
      <% if @range == "大会名" %>
        <% if @races.count == 0 %>
      　   <p>該当する大会はありません。</p>
        <% end %>
        <tbody>
          <% @races.each do |race| %>
            <tr>
              <td><%=link_to race.name,review_path(race)%></td>
            </tr>
          <% end %>
        </tbody>
      <!--検索対象モデルがend_userの時 -->
      <% else @end_users == "User名" %>
        <% if @end_users.count == 0 %>
          <p>該当するユーザーはありません。</p>
        <% end %>
        <tbody>
          <% @end_users.each do |end_user| %>
            <tr>
              <td><%=link_to end_user.name, end_user_path(end_user)%></td>
            </tr>
          <% end %>
        </tbody>
      <% end %>
    </table>
  </div>